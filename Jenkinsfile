pipeline {
    agent any

   

        stage('Test') {
            steps {
                echo 'Running tests... (simulated)'
            }
        }

        stage('Deliver') {
            steps {
                echo 'Delivering the application... (simulated)'
            }
        }

        stage('End') {
            steps {
                echo 'Pipeline finished successfully.'
            }
        }
    }
}
