pipeline {
    agent any

    stages {
        stage('Checkout SCM') {
            steps {
                echo 'Checking out code...'
          
            }
        }

        stage('Build') {
            steps {
                echo 'Building the project... (simulated)'
            }
        }

        stage('Test') {
            steps {
                echo 'Running tests... (simulated)'
            }
        }


        stage('End') {
            steps {
                echo 'Pipeline finished successfully.'
            }
        }
    }
}
